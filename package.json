{
  "name": "genmon4-backend",
  "version": "1.0.0",
  "description": "Backend service for GenMon4 contest platform with real-time leaderboard updates",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "contest",
    "coding",
    "leaderboard",
    "real-time",
    "judge0",
    "websocket"
  ],
  "author": "GenMon4 Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^5.7.0",
    "morgan": "^1.10.0",
    "redis": "^4.6.7",
    "socket.io": "^4.7.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "eslint": "^8.45.0",
    "eslint-config-node": "^4.1.0",
    "jest": "^29.6.2",
    "mongodb-memory-server": "^8.13.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
